{
    "name": "mozu/api",
    "description": "SDK for Mozu Rest APIs",
    "type": "library",
    "homepage": "https://developer.mozu.com/sdks",
    "keywords": ["mozu","sdk","commerce"],
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "6.0.2",
        "guzzlehttp/psr7": "~1.1",
        "guzzlehttp/promises": "~1.0",
        "apache/log4php": "2.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.2"
    },
    "autoload": {
        "psr-4": {
            "Mozu\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mozu\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.21.x-dev"
        }
    }
}
